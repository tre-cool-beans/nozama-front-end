<h1>Past Orders</h1>
<hr>
<div id='messages'>
  Not a message.
</div>
<hr>

<div class="container">
  {{#each this}}
  <div class="row-0">
    <div class="col-xs-12">
      <p> Ordered on: {{createdAt}}</p>
    </div>

        {{#each cart}}
        <div class="col-0-0">
          <div class="media">
            <div class="media-left">
              <a href="#">
                <img class="media-object" src="{{this.image_url}}" alt="...">
              </a>
            </div>
            <div class="media-body">
              <h4 class="media-heading">{{this.title}}</h4>
              <p>{{this.description}}</p>
            </div>
          </div>
        </div>

          <div class="col-0-1">
            <p>{{this.price}}</p>
          </div>
        {{/each}}




      <div class="col-0-0">
        <p> Total:</p>
      </div>

      <div class="col-0-1">
         <p>{{total_price}}</p>
      </div>

      </div>

    </div>
  {{/each}}
</div>
